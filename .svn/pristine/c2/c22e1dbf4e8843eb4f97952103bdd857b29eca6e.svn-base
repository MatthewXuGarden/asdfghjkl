// alarms.js


function onSelectAlarmTable(at)
{
	var idLine = document.getElementById("idLine").value;
	top.frames['body'].frames['bodytab'].location.href = top.frames['manager'].getDocumentBase()
		+ "mobile/Alarms.jsp;"
		+ top.frames['manager'].getSessionId()
		+ "?at=" + at + "&idline=" + idLine;
}


function onSelectLine(idLine)
{
	var at = document.getElementById("alarmTable").value;
	top.frames['body'].frames['bodytab'].location.href = top.frames['manager'].getDocumentBase()
		+ "mobile/Alarms.jsp;"
		+ top.frames['manager'].getSessionId()
		+ "?at=" + at + "&idline=" + idLine;
}
