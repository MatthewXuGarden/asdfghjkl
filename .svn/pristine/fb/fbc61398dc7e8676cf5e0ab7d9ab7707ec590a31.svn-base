.tab {
    BORDER-RIGHT: buttonshadow 1px solid;
    PADDING-RIGHT: 3px;
    BORDER-TOP: buttonhighlight 1px solid;
    PADDING-LEFT: 3px;
    BACKGROUND: #AAAAAA;
    PADDING-BOTTOM: 3px;
    BORDER-LEFT: buttonhighlight 1px solid;
    CURSOR: pointer;
    PADDING-TOP: 3px;
    BORDER-BOTTOM: buttonhighlight 1px solid;
}

.tab_mouse_over {
    BORDER-RIGHT: buttonshadow 1px solid;
    PADDING-RIGHT: 3px;
    BORDER-TOP: buttonhighlight 1px solid;
    PADDING-LEFT: 3px;
    BACKGROUND: #BBBBBB;
    PADDING-BOTTOM: 3px;
    BORDER-LEFT: buttonhighlight 1px solid;
    CURSOR: pointer;
    PADDING-TOP: 3px;
    BORDER-BOTTOM: buttonhighlight 1px solid;
}
.tab_mouse_out {
    BORDER-RIGHT: buttonshadow 1px solid;
    PADDING-RIGHT: 3px;
    BORDER-TOP: buttonhighlight 1px solid;
    PADDING-LEFT: 3px;
    BACKGROUND: #555555;
    PADDING-BOTTOM: 3px;
    BORDER-LEFT: buttonhighlight 1px solid;
    CURSOR: pointer;
    PADDING-TOP: 3px;
    BORDER-BOTTOM: buttonhighlight 1px solid
}
.tab_mouse_click {
    BORDER-RIGHT: buttonshadow 1px solid;
    PADDING-RIGHT: 3px;
    BORDER-TOP: buttonhighlight 1px solid;
    PADDING-LEFT: 3px;
    BACKGROUND: #BBBBBB;
    PADDING-BOTTOM: 3px;
    BORDER-LEFT: buttonhighlight 1px solid;
    CURSOR: pointer;
    PADDING-TOP: 3px;
    BORDER-BOTTOM: buttonhighlight 1px solid
}
.page_selected {
    BORDER-RIGHT: buttonshadow 1px solid;
    PADDING-RIGHT: 3px;
    BORDER-TOP: buttonhighlight 1px solid;
    PADDING-LEFT: 3px;
    BACKGROUND: #EEEEEE;
    PADDING-BOTTOM: 3px;
    BORDER-LEFT: buttonhighlight 1px solid;
    CURSOR: pointer;
    PADDING-TOP: 3px;
    BORDER-BOTTOM: buttonhighlight 1px solid
}

.Tab_Area {
