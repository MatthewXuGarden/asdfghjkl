.toprightcorner {
	margin-top: -12px; /* to level of top-left corner */
	margin-left: 12px; /* clip left corner */
	background-image: url('../images/pvpro20/topright_corner.png'); 
	background-position: 100% -12px; /* show bg-image starting at the right */
	height: 12px; 
	font-size: 2px;	
}

.bottomleftcorner {
	clear: left;
	margin-right: 12px; /* clip right corner */
	background-image: url('../images/pvpro20/bottomleft_corner.png');
	background-position: 0 -12px; /* show under half of the image */
	height: 12px;
	font-size: 2px;
}
.bottomrightcorner {
	margin-top: -12px; /* to level of bottom-left corner */
	margin-left: 12px; /* clip left corner */
	background-image: url('../images/pvpro20/bottomright_corner.png'); 
	background-position: 100% -12px; /* under half, right side */
	height: 12px; 
	font-size: 2px;
}

