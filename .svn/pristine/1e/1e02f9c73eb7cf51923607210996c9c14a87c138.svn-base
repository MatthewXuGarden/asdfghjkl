package com.carel.supervisor.presentation.bean;

import com.carel.supervisor.dataaccess.db.DataBaseException;

public class BookletSiteInfoBean {
	private String siteName = "";
	private String siteValue = "";

	public BookletSiteInfoBean() {
	}

	public BookletSiteInfoBean(String field, String value)
			throws DataBaseException {
		this.siteName = field;
		this.siteValue = value;
	}

	public String getSiteName() {
		return siteName;
	}

	public String getSiteValue() {
		return siteValue;
	}

	public void setSiteName(String siteName) {
		this.siteName = siteName;
	}

	public void setSiteValue(String siteValue) {
		this.siteValue = siteValue;
	}

}
