package com.carel.supervisor.dispatcher.plantwatch1;

public class VarInteger {
	private int val = 0;
	VarInteger(int i)
	{
		val = i;
	}
	/**
	 * @return Returns the val.
	 */
	public int getVal() {
		return val;
	}
	/**
	 * @param val The val to set.
	 */
	public void setVal(int val) {
		this.val = val;
	}
}
