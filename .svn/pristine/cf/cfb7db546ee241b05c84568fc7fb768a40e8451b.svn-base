package com.carel.supervisor.base.system;


// attribute provided by SanKitW
public class SanKitAttr {
	public String id			= "000";
	public String description	= "";
	public Integer flag			= 0;
	public Integer value		= 0;
	public Integer worst		= 0;
	public Integer thres		= 0;
	public String raw_value		= "";
	
	public String toString()
	{
		return id
			+ ", " + description
			+ ", " + String.format("%02X", flag)
			+ ", " + value
			+ ", " + worst
			+ ", " + thres
			+ ", " + raw_value;
	}
}
