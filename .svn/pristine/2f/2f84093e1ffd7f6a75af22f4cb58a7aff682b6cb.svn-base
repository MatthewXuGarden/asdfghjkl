package com.carel.supervisor.remote.engine.connection;

public class Connection 
{
	private String modem = "";
	private String type = "";
	private String client = "";
	private String ipLocale = "";
	private String ipRemoto = "";
	
	public Connection(String idModem,String type,String idClient,String locale,String remoto) {
		this.modem = idModem;
		this.type = type;
		this.client = idClient;
		this.ipLocale = locale;
		this.ipRemoto = remoto;
	}

	public String getClient() {
		return client;
	}

	public String getModem() {
		return modem;
	}

	public String getType() {
		return type;
	}

	public String getIpLocale() {
		return ipLocale;
	}

	public String getIpRemoto() {
		return ipRemoto;
	}
	
	public void setClient(String id) {
		this.client = id;
	}
}
