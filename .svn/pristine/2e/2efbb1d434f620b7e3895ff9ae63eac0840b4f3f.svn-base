<html>
<head>

	<meta http-equiv="pragma" content="no-cache">
  	<meta http-equiv="cache-control" content="no-cache">
  	<meta http-equiv="expires" content="0">
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">

<script>
function loadAction()
{
	var pfile = "";
	
	if(parent.document.URL)
	{
		var callUrl = parent.document.URL;
		if(callUrl != null && callUrl.indexOf("?") != -1)
		{
			pfile = callUrl.substring((callUrl.indexOf("?")+1));
			document.getElementById("path").value = pfile;
			document.getElementById('stamparep').submit();
		}
	}
}
</script>
</head>
<body onload="loadAction();">
<form action="../../servlet/document" id="stamparep" method="post">
<input type="hidden" name="path" id="path" value=""/>	
</form>	
</body>
</html>
