package com.carel.supervisor.field.types;

public class ExtUnitInfoT
{
    private short actualSize;
    private short type;
    private short fwRelease;
    private short protocolCap;
    private short hwCode;
    private byte exportIfaceCode;
    private byte applicationRelease;
    private short applicationCode;
    private short customerCode;
    private byte uSMaxVar;
    private byte bMaxVar;
    private short returnCode = 0;

    public ExtUnitInfoT(short actualSize, short type, short fwRelease, short protocolCap,
        short hwCode, byte exportIfaceCode, byte applicationRelease, short applicationCode,
        short customerCode, byte uSMaxVar, byte bMaxVar, short returncode)
    {
        this.actualSize = actualSize;
        this.type = type;
        this.fwRelease = fwRelease;
        this.protocolCap = protocolCap;
        this.hwCode = hwCode;
        this.exportIfaceCode = exportIfaceCode;
        this.applicationRelease = applicationRelease;
        this.applicationCode = applicationCode;
        this.customerCode = customerCode;
        this.uSMaxVar = uSMaxVar;
        this.bMaxVar = bMaxVar;
        this.returnCode = returncode;
    }

    /**
     * @return Returns the actualSize.
     */
    public short getActualSize()
    {
        return actualSize;
    }

    /**
     * @param actualSize The actualSize to set.
     */
    public void setActualSize(short actualSize)
    {
        this.actualSize = actualSize;
    }

    /**
     * @return Returns the applicationCode.
     */
    public short getApplicationCode()
    {
        return applicationCode;
    }

    /**
     * @param applicationCode The applicationCode to set.
     */
    public void setApplicationCode(short applicationCode)
    {
        this.applicationCode = applicationCode;
    }

    /**
     * @return Returns the applicationRelease.
     */
    public byte getApplicationRelease()
    {
        return applicationRelease;
    }

    /**
     * @param applicationRelease The applicationRelease to set.
     */
    public void setApplicationRelease(byte applicationRelease)
    {
        this.applicationRelease = applicationRelease;
    }

    /**
     * @return Returns the bMaxVar.
     */
    public byte getBMaxVar()
    {
        return bMaxVar;
    }

    /**
     * @param maxVar The bMaxVar to set.
     */
    public void setBMaxVar(byte maxVar)
    {
        bMaxVar = maxVar;
    }

    /**
     * @return Returns the customerCode.
     */
    public short getCustomerCode()
    {
        return customerCode;
    }

    /**
     * @param customerCode The customerCode to set.
     */
    public void setCustomerCode(short customerCode)
    {
        this.customerCode = customerCode;
    }

    /**
     * @return Returns the exportIfaceCode.
     */
    public byte getExportIfaceCode()
    {
        return exportIfaceCode;
    }

    /**
     * @param exportIfaceCode The exportIfaceCode to set.
     */
    public void setExportIfaceCode(byte exportIfaceCode)
    {
        this.exportIfaceCode = exportIfaceCode;
    }

    /**
     * @return Returns the fwRelease.
     */
    public short getFwRelease()
    {
        return fwRelease;
    }

    /**
     * @param fwRelease The fwRelease to set.
     */
    public void setFwRelease(short fwRelease)
    {
        this.fwRelease = fwRelease;
    }

    /**
     * @return Returns the hwCode.
     */
    public short getHwCode()
    {
        return hwCode;
    }

    /**
     * @param hwCode The hwCode to set.
     */
    public void setHwCode(short hwCode)
    {
        this.hwCode = hwCode;
    }

    /**
     * @return Returns the protocolCap.
     */
    public short getProtocolCap()
    {
        return protocolCap;
    }

    /**
     * @param protocolCap The protocolCap to set.
     */
    public void setProtocolCap(short protocolCap)
    {
        this.protocolCap = protocolCap;
    }

    /**
     * @return Returns the type.
     */
    public short getType()
    {
        return type;
    }

    /**
     * @param type The type to set.
     */
    public void setType(short type)
    {
        this.type = type;
    }

    /**
     * @return Returns the uSMaxVar.
     */
    public byte getUSMaxVar()
    {
        return uSMaxVar;
    }

    /**
     * @param maxVar The uSMaxVar to set.
     */
    public void setUSMaxVar(byte maxVar)
    {
        uSMaxVar = maxVar;
    }

    /**
     * @return Returns the returnCode.
     */
    public short getReturnCode()
    {
        return returnCode;
    }

    /**
     * @param returnCode The returnCode to set.
     */
    public void setReturnCode(short returnCode)
    {
        this.returnCode = returnCode;
    }
}
